{
	"name": "senutila",
	"version": "0.2.3",
	"description": "A Discord bot framework focused on providing a great developer experience. Also better than discord.js.",
	"main": "lib/index.js",
	"files": [
		"lib",
		"package.json"
	],
	"scripts": {
		"build": "rimraf lib/*&&tsc",
		"dev": "tsc --watch --incremental",
		"build-example": "rimraf example/*.js&&tsc --project tsconfig.example.json",
		"dev-example": "rimraf example/*.js&&tsc --project tsconfig.example.json --watch",
		"build-api-docs": "rimraf api/*&&typedoc"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/edazpotato/senutila.git"
	},
	"keywords": [
		"discord",
		"bot",
		"robotics",
		"cli",
		"backend"
	],
	"author": "https://github.com/edazpotato",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/edazpotato/senutila/issues"
	},
	"homepage": "https://github.com/edazpotato/senutila#readme",
	"dependencies": {
		"@discordjs/collection": "^0.2.1",
		"@discordjs/rest": "^0.1.0-canary.0",
		"bufferutil": "^4.0.3",
		"centra": "^2.5.0",
		"chalk": "^4.1.1",
		"discord-api-types": "^0.24.0-next.43cc755.1631062715",
		"dotenv": "^10.0.0",
		"erlpack": "^0.1.3",
		"nodejs-snowflake": "^1.6.2",
		"tweetnacl": "^1.0.3",
		"utf-8-validate": "^5.0.5",
		"ws": "^8.0.0",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@types/centra": "^2.2.0",
		"@types/node": "^16.4.12",
		"@types/ws": "^7.4.7",
		"rimraf": "^3.0.2",
		"typedoc": "^0.22.5",
		"typescript": "^4.3.5"
	}
}
